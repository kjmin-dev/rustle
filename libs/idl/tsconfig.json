{
    "compilerOptions": {
        "target": "ES2020",
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "esModuleInterop": true,
        "declaration": true,
        "outDir": "./gen/ts",
        "rootDir": "./gen/ts",
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["gen/ts/**/*.ts"],
    "exclude": ["node_modules"]
}
